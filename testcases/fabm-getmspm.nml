!-------------------------------------------------------------------------------
! fabm_nml
!-------------------------------------------------------------------------------
! models             [string array]
!                     a comma-separated list of one or model model names
!-------------------------------------------------------------------------------
&fabm_nml
   models = 'iow_getmspm'
/

!-------------------------------------------------------------------------------
! iow_getmspm - the GETM SPM model
!-------------------------------------------------------------------------------

&iow_getmspm
   c_init=1.0
   shading=1.0,
   mass_sed_init=100.
   tauc_erosion=0.02
   tauc_sedimentation=0.02
   erosion_const=0.01
   ws=-10.0
/
